{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app-eforms-notice-editor": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/app-eforms-notice-editor",
      "sourceRoot": "projects/app-eforms-notice-editor/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "allowedCommonJsDependencies": [
              "hash-sum"
            ],
            "outputPath": "dist/app-eforms-notice-editor",
            "index": "projects/app-eforms-notice-editor/src/index.html",
            "browser": "projects/app-eforms-notice-editor/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/app-eforms-notice-editor/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/app-eforms-notice-editor/src/favicon.ico",
              "projects/app-eforms-notice-editor/src/assets",
              "projects/app-eforms-notice-editor/src/styles",
              {
                "glob": "**/*",
                "input": "projects/app-eforms-notice-editor/src/fonts/",
                "output": "/fonts/"
              }
            ],
            "styles": [
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/themes/nova/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/@maggioli-design-system/icons/dist/mgg-icons.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "50kb",
                  "maximumError": "100kb"
                }
              ],
              "outputHashing": "all",
              "outputPath": {
                "base": "dist/app-eforms-notice-editor",
                "browser": ""
              }
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "app-eforms-notice-editor:build:production"
            },
            "development": {
              "buildTarget": "app-eforms-notice-editor:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "app-eforms-notice-editor:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/app-eforms-notice-editor/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/app-eforms-notice-editor/src/favicon.ico",
              "projects/app-eforms-notice-editor/src/assets"
            ],
            "styles": [
              "projects/app-eforms-notice-editor/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "sdk-commons": {
      "root": "projects/sdk-commons",
      "sourceRoot": "projects/sdk-commons/src",
      "projectType": "library",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/sdk-commons/tsconfig.lib.json",
            "project": "projects/sdk-commons/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/sdk-commons/tsconfig.lib.prod.json"
            }
          }
        }
      }
    },
    "sdk-controls": {
      "root": "projects/sdk-controls",
      "sourceRoot": "projects/sdk-controls/src",
      "projectType": "library",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/sdk-controls/tsconfig.lib.json",
            "project": "projects/sdk-controls/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/sdk-controls/tsconfig.lib.prod.json"
            }
          }
        }
      }
    },
    "sdk-widgets": {
      "root": "projects/sdk-widgets",
      "sourceRoot": "projects/sdk-widgets/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/sdk-widgets/tsconfig.lib.json",
            "project": "projects/sdk-widgets/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/sdk-widgets/tsconfig.lib.prod.json"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}